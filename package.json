{
  "private": true,
  "name": "",
  "version": "",
  "description": "<img src=\"https://raw.githubusercontent.com/huwb/crest-oceanrender/master/logo/crest-oceanrender-logotype1.png\" width=\"214\">",
  "docsifytopdf": {
    "contents": [
      "docs/_sidebar.md"
    ],
    "pathToPublic": "pdf/readme.pdf",
    "removeTemp": true,
    "emulateMedia": "print",
    "pathToDocsifyEntryPoint": "./docs",
    "mainMdFilename": "README.md",
    "$docsify": {
      "hdrp": {
        "select": {
          "useSelectHeadingComment": true,
          "sync": true,
          "selected": {
            "rendering-pipeline": "high definition"
          }
        }
      },
      "urp": {
        "select": {
          "useSelectHeadingComment": true,
          "sync": true,
          "selected": {
            "rendering-pipeline": "universal"
          }
        }
      },
      "brp": {
        "select": {
          "useSelectHeadingComment": true,
          "sync": true,
          "selected": {
            "rendering-pipeline": "built-in"
          }
        }
      }
    },
    "pdfOptions": {
      "margin": {
        "top": "10mm",
        "left": "10mm",
        "right": "9.5mm",
        "bottom": "9.5mm"
      },
      "displayHeaderFooter": true,
      "headerTemplate": " ",
      "footerTemplate": "<style> .pageFooter { font-size:12px; margin-left:auto; margin-right: 9.5mm;}</style> <span class='pageFooter'><span class='pageNumber'></span> / <span class='totalPages'></span></span>",
      "format": "A4"
    }
  },
  "scripts": {
    "pdf:hdrp": "docsify-pdf-converter hdrp",
    "pdf:urp": "docsify-pdf-converter urp",
    "pdf:brp": "docsify-pdf-converter brp",
    "push": "git push docsify test/docsify:master"
  },
  "devDependencies": {
    "docsify-pdf-converter": "github:daleeidd/docsify-to-pdf-converter#injectable-settings"
  }
}
